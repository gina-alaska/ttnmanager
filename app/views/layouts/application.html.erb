<!DOCTYPE html>
<html>
<head>
  <title>Atnmanager</title>
  <%= stylesheet_link_tag 'screen.css', :media => 'screen, projection' %>
  <%= stylesheet_link_tag 'print.css', :media => 'print' %>

  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body class="bp atn">
<div id="container">
  <div id="header"><%= render 'layouts/header' %></div>
  <div id="footer"><%= render 'layouts/footer' %></div>
  <div id="content">
    <div class="flash">
      <%= raw flash_messages -%>
    </div>
    <%= yield %>
  </div>
</div>
</body>
</html>
